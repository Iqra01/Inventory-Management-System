@model IEnumerable<eadproj.Models.inventitem>

<html>
<head>
    <title>
        TOYS
    </title>
    <style>
        th, td {
            border: 1px inset black;
        }

        table {
            border: 1px outset black;
        }
    </style>
</head>
<body>
    <h1> TOYS </h1>
    <table>
        <tr>
            <th>Name</th>
            <th>Brand </th>
            <th>Price</th>
            <th>Description</th>
            <th>EDIT</th>
        </tr>

        @foreach (var s in Model)
        {
             <tr>
                 <td> @s.Name   </td>
                 <td> @s.Brand </td>
                 <td> @s.Price</td>
                 <td> @s.Description</td>
                 <td> <a href="/Home/EditITEM/@s.Id"> Edit </a>  </td>
             </tr>
        }
    </table>
</body>
</html>